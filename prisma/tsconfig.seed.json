{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES2021",
    "outDir": "../dist/prisma",
    "rootDir": ".",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": false,
    "sourceMap": false,
    "resolveJsonModule": true
  },
  "include": ["seed.ts", "seeds/**/*.ts"],
  "exclude": ["node_modules"]
}
